@using RSFrontEnd.Models;

<div class="container">
<div class="row">

@foreach (var artist in artists)
{
    <div class="col-sm-12 col-md-6 col-lg-4 col-xl-3">
    <ArtistSummary artist="artist"></ArtistSummary>
    </div>
}


</div>
</div>



@code {

    [Parameter]
    public List<Artist> artists { get; set; }

}
